{
    "minimum-stability": "dev",
    "name": "horde/kolab_format",
    "description": "Kolab data format library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Kolab_Format",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Gunnar Wrobel",
            "email": "p@rdus.de",
            "role": "lead"
        }
    ],
    "time": "2022-08-16",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/util": "^3 || dev-FRAMEWORK_6_0",
        "ext-dom": "*"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "horde/support": "^3 || dev-FRAMEWORK_6_0",
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Kolab_Format": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Kolab\\Format\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}